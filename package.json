{
  "name": "wplace-stat",
  "version": "1.0.0",
  "type": "module",
  "main": "server/index.js",
  "scripts": {
    "build": "webpack --mode production",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:build\"",
    "dev:build": "webpack --mode development --watch",
    "dev:server": "nodemon server/index.js",
    "start": "node server/index.js",
    "serve": "npx serve -s dist -l 3001",
    "pm2:start": "npm run build && pm2 start ecosystem.config.js --env production",
    "pm2:stop": "pm2 stop wplace-stat",
    "pm2:restart": "pm2 restart wplace-stat",
    "pm2:delete": "pm2 delete wplace-stat"
  },
  "dependencies": {
    "@types/node-fetch": "^2.6.13",
    "express": "^4.18.2",
    "node-fetch": "^3.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.5.0",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "concurrently": "^9.2.1",
    "css-loader": "^6.8.1",
    "html-webpack-plugin": "^5.5.3",
    "nodemon": "^3.0.1",
    "sass-embedded": "^1.69.5",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.4.4",
    "typescript": "^5.1.6",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "author": "",
  "license": "ISC",
  "description": ""
}
